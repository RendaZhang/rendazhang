{
  "name": "newastrarenda",
  "type": "module",
  "version": "0.0.1",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro}": [
      "eslint src/ astro.config.ts eslint.config.ts --ext .js,.jsx,.ts,.tsx,.astro --fix-dry-run --cache --cache-location .eslintcache"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "validate-assets": "tsx scripts/validate_assets.ts",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "sync": "astro sync",
    "generate-index": "tsx scripts/generateIndex.ts",
    "generate-hero": "tsx scripts/generate-hero.ts",
    "update-docs": "tsx scripts/update_docs.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src astro.config.ts eslint.config.ts",
    "lint:fix": "eslint src astro.config.ts eslint.config.ts --fix",
    "format": "prettier --write src scripts astro.config.ts eslint.config.ts tsconfig.json package.json",
    "sync-readme": "tsx scripts/sync_readme.ts",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@sentry/astro": "^10.0.0",
    "@sentry/react": "^10.0.0",
    "@sentry/vite-plugin": "^4.0.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.12.8",
    "dompurify": "^3.2.6",
    "dotenv-expand": "^12.0.2",
    "highlight.js": "^11.11.1",
    "marked": "^16.0.0",
    "mermaid": "^11.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/ts-plugin": "^1.10.4",
    "@eslint/js": "^9.31.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/postcss-import": "^14.0.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.31.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.4",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "stylelint-order": "^7.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
